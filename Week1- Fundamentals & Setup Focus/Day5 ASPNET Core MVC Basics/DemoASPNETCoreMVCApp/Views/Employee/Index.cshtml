@model List<DemoASPNETCoreMVCApp.Models.Employee>

<h2>Employee List</h2>
<table>
    <thead>
        <tr><th>Name</th><th>Salary</th></tr>
    </thead>
    <tbody>
    @foreach (var emp in Model)
    {
        <tr>
            <td>@emp.Name</td>
            <td>@emp.Salary</td>
        </tr>
    }
    </tbody>
</table>